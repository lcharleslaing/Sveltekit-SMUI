{
	"name": "sveltekit-smui",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev --port 3001",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"smuiprepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@mdi/js": "^6.5.95",
		"@playwright/test": "^1.19.1",
		"@smui/button": "^6.0.0-beta.15",
		"@smui/drawer": "^6.0.0-beta.15",
		"@smui/form-field": "^6.0.0-beta.15",
		"@smui/icon-button": "^6.0.0-beta.15",
		"@smui/switch": "^6.0.0-beta.15",
		"@smui/top-app-bar": "^6.0.0-beta.15",
		"@supabase/supabase-js": "^1.8.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"autoprefixer": "^10.4.2",
		"postcss": "^8.4.5",
		"postcss-load-config": "^3.1.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"smui-theme": "^6.0.0-beta.15",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"tailwindcss": "^3.0.12",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"type": "module"
}
